@model FOMOWizard.Models.FullPayloadUploadViewModal
@{
    ViewData["Title"] = "UploadPayload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Upload Full Payload</h2>
<form asp-action="UploadPayload" enctype="multipart/form-data">
    <div class="form-group row">
        <label asp-for="FullPayload"
               class="col-sm-3 col-form-label">
            New Photo (jpg format)
        </label>
        <div class="col-sm-9">
            <input asp-for="FullPayload" accept="image/jpeg" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-9 offset-sm-3">
            <input type="submit" value="Upload"
                   class="btn btn-primary" />
            <span style="color:red;">@ViewData["Message"]</span>
        </div>
    </div>
</form>

