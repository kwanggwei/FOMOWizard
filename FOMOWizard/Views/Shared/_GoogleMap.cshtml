
<div id="map" style="width: 500px; height: 500px"></div>

<script>
        var map;
        function initMap() {
            var fomoCoords = { lat: 1.2789, lng: 103.8485 };
            map = new google.maps.Map(document.getElementById('map'), {
                center: fomoCoords,
                zoom: 15
            });
            var marker = new google.maps.Marker({
                position: fomoCoords,
                map: map
            });
        }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBB14-3qDRxNzqGy7SXV2KpD4pPINkXpHg&callback=initMap">
</script>

