@model IEnumerable<FOMOWizard.Models.Deployment>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.Count() > 0)
{
    <div class="table-responsive">
        <table id="viewDeployment" class="table table-striped table-bordered">
            <thead class="thead-dark">
                <tr>
                    <th>DeploymentID</th>
                    <th>DeploymentType</th>
                    <th>MID</th>
                    <th>TID</th>
                    <th>Schemes</th>
                    <th>MerchantType</th>
                    <th>SGQRID</th>
                    <th>SGQRVer</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>@item.DeploymentID.ToString()</td>
                    <td>@item.DeploymentType.ToString()</td>
                    <td>@item.MID</td>
                    <td>@item.TID</td>
                    <td>@item.Schemes</td>
                    <td>@item.MerchantType</td>
                    <td>@item.SGQRID</td>
                    <td>@item.SGQRVersion</td>
                    <td><a asp-action="Edit" asp-controller="Staff" asp-route-id="@item.DeploymentID">Edit</a> |</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
}

else
{
    <span style="color:red">No record found!</span>
}
