@model IEnumerable<FOMOWizard.Models.Deployment>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.ToList().Count > 0)
{
    <div class="table-responsive">
        <table id="viewDeployment" class="table table-striped table-bordered">
            <thead class="thead-dark">
                <tr>
                    <th>DeploymentID</th>
                    <th>DeploymentType</th>
                    <th>MID</th>
                    <th>TID</th>
                    <th>Schemes</th>
                    <th>MerchantType</th>
                    <th>SGQRID</th>
                    <th>SGQRVer</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.DeploymentID.ToString()</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
}

else
{
    <span style="color:red">No record found!</span>
}
